            <apex:actionFunction action="{!Submit2}" name="submit">
            </apex:actionFunction>

            <apex:actionFunction action="{!Cancel}" name="cancel">
            </apex:actionFunction>

			<apex:outputpanel id="wrapperBlock">

			</apex:outputpanel>
			<div class="slds-page-header">
		    	<div class="slds-grid">
		        	<div class="slds-col slds-has-flexi-truncate">
